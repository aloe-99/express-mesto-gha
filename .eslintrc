{
  "env": {
    "browser": "true",
    "commonjs": "true",
    "es2021": "true"
  },
  "extends": ["plugin:react/recommended", "airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "off"
  }
}
